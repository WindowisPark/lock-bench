  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "legend": {
          "calcs": [],
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "targets": [
        {
          "expr": "sum by (thread_model, lock_strategy) (rate(lockbench_experiment_run_count_total{thread_model=~\"$thread_model\", lock_strategy=~\"$lock_strategy\"}[1m]))",
          "legendFormat": "{{thread_model}}/{{lock_strategy}}",
          "refId": "A"
        }
      ],
      "title": "Experiment Runs Per Min",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "reqps"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "id": 2,
      "options": {
        "legend": {
          "calcs": [],
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "targets": [
        {
          "expr": "sum by (thread_model, lock_strategy) (rate(lockbench_experiment_request_success_count_total{thread_model=~\"$thread_model\", lock_strategy=~\"$lock_strategy\"}[1m]))",
          "legendFormat": "success {{thread_model}}/{{lock_strategy}}",
          "refId": "A"
        },
        {
          "expr": "sum by (thread_model, lock_strategy) (rate(lockbench_experiment_request_failure_count_total{thread_model=~\"$thread_model\", lock_strategy=~\"$lock_strategy\"}[1m]))",
          "legendFormat": "failure {{thread_model}}/{{lock_strategy}}",
          "refId": "B"
        }
      ],
      "title": "Request Throughput (Success/Failure)",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "max": 100,
          "min": 0,
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "id": 3,
      "options": {
        "legend": {
          "calcs": [],
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "targets": [
        {
          "expr": "100 * sum by (thread_model, lock_strategy) (rate(lockbench_experiment_request_failure_count_total{thread_model=~\"$thread_model\", lock_strategy=~\"$lock_strategy\"}[1m])) / clamp_min(sum by (thread_model, lock_strategy) (rate(lockbench_experiment_request_success_count_total{thread_model=~\"$thread_model\", lock_strategy=~\"$lock_strategy\"}[1m]) + rate(lockbench_experiment_request_failure_count_total{thread_model=~\"$thread_model\", lock_strategy=~\"$lock_strategy\"}[1m])), 1e-9)",
          "legendFormat": "{{thread_model}}/{{lock_strategy}}",
          "refId": "A"
        }
      ],
      "title": "Failure Ratio %",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "ms"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "id": 4,
