      "options": {
        "legend": {
          "calcs": [],
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "targets": [
        {
          "expr": "sum by (thread_model, lock_strategy) (rate(lockbench_experiment_throughput_req_per_sec_sum{thread_model=~\"$thread_model\", lock_strategy=~\"$lock_strategy\"}[5m])) / clamp_min(sum by (thread_model, lock_strategy) (rate(lockbench_experiment_throughput_req_per_sec_count{thread_model=~\"$thread_model\", lock_strategy=~\"$lock_strategy\"}[5m])), 1e-9)",
          "legendFormat": "{{thread_model}}/{{lock_strategy}}",
          "refId": "A"
        }
      ],
      "title": "Avg Throughput (req/s)",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "ops"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
